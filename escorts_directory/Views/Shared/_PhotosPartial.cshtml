@using escorts_directory.Models.VM
@model EscortWithPhoto

@for (int i = 1; i <= 11; i++)
{
    var filePath = Url.Content($"~/images/Profiles/{Model.Escort.Name}_{Model.Escort.Id}_{i}.png");
    <div class="col-lg-6">
        <div class="Article_imageBlock">
            <img src="@filePath"
                 alt="@Model.Escort.Name"
                 title="@Model.Escort.Name"
                 class="img-fluid"
                 onerror="this.style.display='none'" />
        </div>
    </div>
}
